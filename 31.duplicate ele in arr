#include <stdio.h>
int main() 
{
    int i, j;
    int arr[] = {3, 5, 7, 2, 8, 5, 7};
    int n = sizeof(arr) / sizeof(arr[0]);
    printf("Duplicate elements in the array: ");
    for(i = 0; i < n; i++) 
	{
        for(j = i + 1; j < n; j++) 
		{
            if(arr[i] == arr[j]) 
			{
                printf("%d ", arr[i]);
                break;
            }
        }
    }
    return 0;
}
